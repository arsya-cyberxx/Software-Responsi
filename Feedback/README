FORMAT PULL SERVER
data_to_pull = {
    "user_ID": "111",
    "user_voucherEligible": True,
    "cart": {
        "Product C": {
            "rating_total": 40,
            "rating_frequency": 7
        },
        "Product B": {
            "rating_total": 40,
            "rating_frequency": 21
        }
    },
    "restock_frequency": {
        "A": 40,
        "B": 5,
        "C": 2,
        "D": 10
    }
}

json_data = json.dumps(data_to_pull).encode('utf-8')


